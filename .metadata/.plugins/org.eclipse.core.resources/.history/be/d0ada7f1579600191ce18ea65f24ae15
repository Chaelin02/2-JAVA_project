package Quize;

public class english {

	public static void main(String[] args) {
		int S , O , T;
		
		for(S=1; S<=9; S++) {
			for(O=0; O<=9; O++) {
				for(T=0; T<=9; T++) {
					if((S*100 + O*10 + T) - (S*10 + O) == (T * 10 + T)) {
						System.out.println(S + "");
					}
			}
		}
		
		System.out.println("SO + SO = TOO \n" + (S+O) + (S+O));
		System.out.println();
	}

}
